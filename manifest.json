{
  "name": "like2iine for chrome extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "change the love button of Twitter",
  "web_accessible_resources": ["like2iine.css"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["like2iine.css"]
    }
  ],
  "icons": { "128": "icon.png" }
}
